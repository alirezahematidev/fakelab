{
  "name": "fakelab",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "A fast, easy-config mock server for frontend developers.",
  "main": "lib/main.js",
  "module": "lib/main.js",
  "types": "lib/main.d.ts",
  "exports": {
    ".": {
      "types": "./lib/main.d.ts",
      "default": "./lib/main.js"
    },
    "./runtime": {
      "types": "./lib/runtime.d.ts",
      "node": "./lib/runtime.js",
      "default": "./lib/runtime.js"
    },
    "./database": {
      "types": "./lib/database.d.ts",
      "default": "./lib/database.js"
    },
    "./type-utils": {
      "types": "./lib/type-utils.d.ts"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alirezahematidev/fakelab.git"
  },
  "homepage": "https://github.com/alirezahematidev/fakelab",
  "bugs": {
    "url": "https://github.com/alirezahematidev/fakelab/issues"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "prepack": "yarn build",
    "release": "release-it --ci",
    "serve": "yarn build && node --import=tsx lib/cli.js serve",
    "lint": "yarn eslint --config eslint.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "bin",
    "lib",
    "package.json"
  ],
  "keywords": [
    "typescript",
    "mock-server",
    "mock api",
    "faker",
    "fakelab"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@release-it/conventional-changelog": "^10.0.2",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.6",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/fs-extra": "^11.0.4",
    "@types/is-glob": "^4.0.4",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.16",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "execa": "^9.6.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "release-it": "^19.0.6",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "uglify-js": "^3.19.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "bundle-require": "^5.1.0",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "esbuild": "^0.27.2",
    "express": "^5.2.1",
    "express-ejs-layouts": "^2.5.1",
    "fast-glob": "^3.3.3",
    "figlet": "^1.9.4",
    "fs-extra": "^11.3.2",
    "is-glob": "^4.0.3",
    "joycon": "^3.1.1",
    "lowdb": "^7.0.1",
    "mitt": "^3.0.1",
    "picocolors": "^1.1.1",
    "qs": "^6.14.0",
    "quicktype-core": "23.2.6",
    "ts-morph": "^27.0.2"
  },
  "bin": {
    "fakelab": "bin/run.js"
  }
}
