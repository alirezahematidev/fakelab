<aside class="sidebar">
  <a class="logo" href="/"><img src="/icons/logo.svg" width="20px" height="20px" fetchpriority="high" loading="eager" /><span>Fakelab</span></a>
  <div class="sidebar_content">
    <div class="database_section">
      <a href="<%= backPath %>" class="database_btn">
        <img src="/icons/arrow-left.svg" width="18px" height="18px" />
        <span>Back</span>
      </a>
    </div>
    <div class="sidebar_content_leading">
      <div class="sidebar_content_indicator_icon"></div>
      <span class="sidebar_content_title">Entities</span>
    </div>
    <div class="sidebar_content_list">
      <% entities.forEach((entity,entityName) => { %>
      <a class="sidebar_content_list_item <%= entityName === name ? 'active' : '' %>" href="/graphql/<%= entityName %>">
        <span class="item_name"><%= entityName %></span>
        <span class="item_filepath"><%= entity.snapshot ? "(snapshot)" : entity.filepath %></span>
      </a>
      <% }) %>
    </div>
  </div>
  <a class="sidebar_docs" href="/faker-cheatsheet">
    <span>FakerJS cheatsheet</span>
    <img class="icon" src="/icons/link.svg" width="18px" height="18px" />
  </a>
  <div class="sidebar_footer">
    <span>app version: <%= version %></span>
  </div>
</aside>
