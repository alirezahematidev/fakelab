<%- include("components/sidebar", {entities, name, version, dbEnabled }) %> <% title = name %>
<main class="main">
  <header class="header">
    <span class="interface_name"><%= name %> <%= suffix %></span>
  </header>
  <div class="preview">
    <div class="preview-loading loading">
      <span class="loader visible"></span><span class="error">Some libraries could not be loaded. check your internet connection and try again.</span>
    </div>
    <%- include("components/toolbar", { address, prefix, name, search, graphqlEnabled }) %> <%- include("components/code", { lang: "json", code: json }) %>
  </div>
</main>
<script src="/js/fakelab.min.js"></script>
